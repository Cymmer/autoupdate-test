    "react-simple-code-editor": "^0.11.0",
    "react-simple-keyboard": "3.2.47",
    "react-syntax-highlighter": "12.2.1",
    "redux": "^4.1.0",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "regenerator-runtime": "^0.13.9",
    "tippy.js": "^6.3.1",
    "use-debounce": "^3.4.2",
    "xterm-addon-attach": "^0.6.0",
